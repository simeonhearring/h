<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html" xmlns:h="urn:import:h.style.g.client.ui">
	<ui:with field="msg" type="h.khall.client.resource.Messages" />
	<ui:with field="res" type="h.khall.client.resource.Resource" />
	<ui:style>
	
	</ui:style>
	<b.gwt:HTMLPanel addStyleNames="row">
		<div class="col-lg-12">
			<div class="ibox-content">
				<div id="wizard">

					<h1>Personal</h1>
					<div class="step-content">
						<div class="text-center m-t-md">
							<h2>Personal Information</h2>
						</div>
						<div class="col-md-4">
							<div class="form-group">
								<label>First</label>
								<input type="text" placeholder="First name" class="form-control" tabindex="1" />
							</div>
							<div class="form-group">
								<label>Suffix</label>
								<input type="text" placeholder="Suffix (Jr., Sr., III)" class="form-control" tabindex="4" />
							</div>
							<div class="form-group">
								<label>Email</label>
								<input type="text" placeholder="Email address" class="form-control" tabindex="7" />
							</div>
						</div>
						<div class="col-md-4 b-r">
							<div class="form-group">
								<label>Middle</label>
								<input type="text" placeholder="Middle name" class="form-control" tabindex="2" />
							</div>
							<div class="form-group">
								<label>Gender</label>
								<input type="text" placeholder="Gender" class="form-control" tabindex="5" />
							</div>
							<div class="form-group">
								<label>Mobile</label>
								<input type="text" placeholder="Mobile phone" class="form-control" tabindex="8" />
							</div>
						</div>
						<div class="col-md-4 b-r">
							<div class="form-group">
								<label>Last</label>
								<input type="email" placeholder="Last name" class="form-control" tabindex="3" />
							</div>
							<div class="form-group">
								<label>Birth</label>
								<input type="text" placeholder="Birth date" class="form-control" tabindex="6" />
							</div>
							<div class="form-group">
								<label>Home</label>
								<input type="text" placeholder="Home phone" class="form-control" tabindex="9" />
							</div>
						</div>
					</div>

					<h1>Address</h1>
					<div class="step-content">
						<div class="text-center m-t-md">
							<h2>Address information</h2>
						</div>
						<div class="col-md-6 b-r">
							<div class="form-group">
								<label>Address (1)</label>
								<input type="text" placeholder="Address line 1" class="form-control" tabindex="1" />
							</div>
							<div class="form-group">
								<label>City</label>
								<input type="text" placeholder="City" class="form-control" tabindex="3" />
							</div>
							<div class="form-group">
								<label>Zip</label>
								<input type="email" placeholder="Zip" class="form-control" tabindex="5" />
							</div>
						</div>
						<div class="col-md-6">
							<div class="form-group">
								<label>Address (2)</label>
								<input type="text" placeholder="Address line 2" class="form-control" tabindex="3" />
							</div>
							<div class="form-group">
								<label>State</label>
								<input type="email" placeholder="State" class="form-control" tabindex="4" />
							</div>
						</div>
					</div>

					<h1>Congregation</h1>
					<div class="step-content">
						<div class="text-center m-t-md">
							<h2>Congregation Information</h2>
						</div>
						<div class="col-md-6 b-r">
							<div class="form-group">
								<label>Field Service Group </label>
								<input type="text" placeholder="Field service group" class="form-control" tabindex="1" />
							</div>
							<div class="form-group">
								<label>Baptized</label>
								<input type="text" placeholder="Baptized" class="form-control" tabindex="3" />
							</div>
							<div class="form-group">
								<label>Roles</label>
								<input type="email" placeholder="Roles" class="form-control" tabindex="5" />
							</div>
							<div class="form-group">
								<label>Emergency</label>
								<input type="email" placeholder="Emergency" class="form-control" tabindex="5" />
							</div>
							<div class="form-group">
								<label>Head Of House</label>
								<input type="email" placeholder="Head of house" class="form-control" tabindex="5" />
							</div>
						</div>
						<div class="col-md-6">
							<div class="form-group">
								<label>Faith</label>
								<input type="text" placeholder="Faith" class="form-control" tabindex="1" />
							</div>
							<div class="form-group">
								<label>Started Publishing</label>
								<input type="text" placeholder="Started publishing" class="form-control" tabindex="3" />
							</div>
							<div class="form-group">
								<label>Categories</label>
								<input type="email" placeholder="Categories" class="form-control" tabindex="5" />
							</div>
							<div class="form-group">
								<label>Children</label>
								<input type="email" placeholder="Children" class="form-control" tabindex="5" />
							</div>
							<div class="form-group">
								<label>Family</label>
								<input type="email" placeholder="Family members" class="form-control" tabindex="5" />
							</div>
						</div>
					</div>

				</div>
			</div>
			<div class="ibox-content">
				<button class="btn btn-sm btn-primary m-t-n-xs" type="submit">
					<strong>Save</strong>
				</button>
			</div>
		</div>
	</b.gwt:HTMLPanel>
</ui:UiBinder>