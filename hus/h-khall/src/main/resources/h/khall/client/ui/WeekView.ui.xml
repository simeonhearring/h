<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html" xmlns:h="urn:import:h.style.g.client.ui" xmlns:k="urn:import:h.khall.client.ui" xmlns:b.tag="urn:import:org.gwtbootstrap3.extras.tagsinput.client.ui"
	xmlns:b.card="urn:import:org.gwtbootstrap3.extras.card.client.ui">
	<ui:with field="msg" type="h.khall.client.resource.Messages" />
	<ui:with field="res" type="h.khall.client.resource.Resource" />
	<ui:style>
	.color {
		border-top-color: #2f4050;
		border-right-color: #2f4050;
	}
	
	.margin {
		margin-bottom: 110px;
	}
	
	.height {
		height: 300px;
	}
	</ui:style>
	<b.gwt:HTMLPanel addStyleNames="ibox-content">
		<div class="row">
			<div class="col-md-12">
				<div class="m-t-md">
					<h3 ui:field="mWeekOf" />
				</div>
			</div>
		</div>
		<b.html:Hr addStyleNames="{style.color}" />
		<div class="row">
			<div class="col-md-4 b-r {style.color}">
				<form role="form">
					<k:AssignView ui:field="mChairman" part="CHAIRMAN" />
					<k:AssignView ui:field="mPrayer1" part="SONG_1" />
					<k:AssignView ui:field="mPrayer2" part="SONG_3" />
					<b.html:Hr addStyleNames="{style.color}" />
					<k:AssignView ui:field="mTreasures" part="TREASURES" />
					<k:AssignView ui:field="mDigging" part="DIGGING" addStyleNames="{style.margin}" />
				</form>
			</div>
			<div class="col-md-8" style="{style.height}">
				<div class="row">
					<div class="col-md-6">
						<k:AssignView ui:field="mLiving1" part="LIVING_1" />
					</div>
					<div class="col-md-6">
						<k:AssignView ui:field="mLiving2" part="LIVING_2" />
					</div>
				</div>
				<k:AssignView ui:field="mCStudy" part="C_BIBLE_STUDY" />
				<b.html:Hr addStyleNames="{style.color}" />
				<div class="row" style="{style.height}">
					<div class="col-md-12">
						<b.card:Card>
							<b.card:Front>
								<b.gwt:HTMLPanel>
									<label>Main Hall</label>
									<k:AssignView ui:field="mReadingM" part="BIBLE_READING" />
									<k:AssignView ui:field="mApply1M" part="APPLY1" ppart="APPLY1" />
									<k:AssignView ui:field="mApply2M" part="APPLY2" ppart="APPLY2" />
									<k:AssignView ui:field="mApply3M" part="APPLY3" ppart="APPLY3" />
								</b.gwt:HTMLPanel>
							</b.card:Front>
							<b.card:Back>
								<b.gwt:HTMLPanel>
									<label>Auxiliary Classroom #1</label>
									<k:AssignView ui:field="mReadingA" part="BIBLE_READING" hall="SECOND" />
									<k:AssignView ui:field="mApply1A" part="APPLY1" ppart="APPLY1" hall="SECOND" />
									<k:AssignView ui:field="mApply2A" part="APPLY2" ppart="APPLY2" hall="SECOND" />
									<k:AssignView ui:field="mApply3A" part="APPLY3" ppart="APPLY3" hall="SECOND" />
								</b.gwt:HTMLPanel>
							</b.card:Back>
						</b.card:Card>
					</div>
				</div>
			</div>
		</div>
	</b.gwt:HTMLPanel>
</ui:UiBinder>