<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html" xmlns:h="urn:import:h.style.g.client.ui">
	<ui:with field="msg" type="h.khall.client.resource.Messages" />
	<ui:with field="res" type="h.khall.client.resource.Resource" />
	<ui:style>
	
	</ui:style>
	<b.gwt:HTMLPanel ui:field="mTop" addStyleNames="ibox float-e-margins">
		<div class="ibox-title">

			<b:Heading ui:field="mHeading" size="H5" />
			<b.html:Span ui:field="mSubHeading" addStyleNames="label label-primary" />

			<div class="ibox-tools">
				<b:Anchor ui:field="mCollaspe" addStyleNames="collapse-link" icon="CHEVRON_UP" />
				<b:Anchor ui:field="mWrench" addStyleNames="dropdown-toggle" icon="WRENCH" dataToggle="DROPDOWN" visible="false" />
				<ul class="dropdown-menu dropdown-user">
					<li>
						<b:Anchor text="Config option 1" />
					</li>
					<li>
						<a href="#">Config option 2</a>
					</li>
				</ul>
				<b:Anchor ui:field="mRemove" addStyleNames="close-link" icon="TIMES" />
			</div>
		</div>
		<b.gwt:HTMLPanel ui:field="mContent" addStyleNames="ibox-content no-padding ease" />
	</b.gwt:HTMLPanel>
</ui:UiBinder>