<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html" xmlns:h="urn:import:h.style.g.client.ui" xmlns:k="urn:import:h.khall.client.ui" xmlns:b.tag="urn:import:org.gwtbootstrap3.extras.tagsinput.client.ui"
	xmlns:b.card="urn:import:org.gwtbootstrap3.extras.card.client.ui">
	<ui:with field="msg" type="h.khall.client.resource.Messages" />
	<ui:with field="res" type="h.khall.client.resource.Resource" />
	<ui:style>
	
	</ui:style>
	<k:CloseableView ui:field="mHead">
		<b.gwt:HTMLPanel ui:field="mTop">
			<b:NavTabs>
				<b:TabListItem ui:field="mT1" active="true" text="Week #1" />
				<b:TabListItem ui:field="mT2" text="Week #2" />
				<b:TabListItem ui:field="mT3" text="Week #3" />
				<b:TabListItem ui:field="mT4" text="Week #4" />
				<b:TabListItem ui:field="mT5" text="Week #5" />
				<b.html:Div addStyleNames="pull-right marginR25">
					<b:Anchor ui:field="mSchedule" icon="PRINT" text="Schedule" marginRight="10" />
					<b:Anchor ui:field="mSlips" icon="PRINT" text="Slips" marginRight="10" />
					<b:Anchor ui:field="mWorksheet" icon="PRINT" text="Worksheet" />
				</b.html:Div>
			</b:NavTabs>
			<b:TabContent>
				<b:TabPane ui:field="mC1" fade="true" in="true" active="true">
					<k:WeekView ui:field="mW0" />
				</b:TabPane>
				<b:TabPane ui:field="mC2" fade="true">
					<k:WeekView ui:field="mW1" />
				</b:TabPane>
				<b:TabPane ui:field="mC3" fade="true">
					<k:WeekView ui:field="mW2" />
				</b:TabPane>
				<b:TabPane ui:field="mC4" fade="true">
					<k:WeekView ui:field="mW3" />
				</b:TabPane>
				<b:TabPane ui:field="mC5" fade="true">
					<k:WeekView ui:field="mW4" />
				</b:TabPane>
			</b:TabContent>
		</b.gwt:HTMLPanel>
	</k:CloseableView>
</ui:UiBinder>