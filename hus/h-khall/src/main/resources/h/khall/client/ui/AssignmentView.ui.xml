<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html" xmlns:h="urn:import:h.style.g.client.ui" xmlns:k="urn:import:h.khall.client.ui" xmlns:b.tag="urn:import:org.gwtbootstrap3.extras.tagsinput.client.ui"
	xmlns:b.card="urn:import:org.gwtbootstrap3.extras.card.client.ui">
	<ui:with field="msg" type="h.khall.client.resource.Messages" />
	<ui:with field="res" type="h.khall.client.resource.Resource" />
	<ui:style>
	
	</ui:style>
	<b.gwt:HTMLPanel addStyleNames="row">
		<div class="col-lg-12">
			<div class="ibox-content">

				<div class="wizard">

					<b.gwt:HTMLPanel ui:field="mSteps" addStyleNames="steps">
						<b.html:UnorderedList addStyleNames="nav nav-pills">
							<b:ListItem ui:field="mStep1l" addStyleNames="current">
								<b:Anchor ui:field="mStep1a" text="1. Personal" />
							</b:ListItem>

							<b:ListItem ui:field="mStep2l" addStyleNames="disabled">
								<b:Anchor ui:field="mStep2a" text="2. Address" />
							</b:ListItem>

							<b:ListItem ui:field="mStep3l" addStyleNames="disabled">
								<b:Anchor ui:field="mStep3a" text="3. Congregation" />
							</b:ListItem>
						</b.html:UnorderedList>
					</b.gwt:HTMLPanel>

					<b.gwt:HTMLPanel ui:field="mStep1c" addStyleNames="content">
						<div class="body">
							<div class="m-t-md">
								<h2 ui:field="mWeekOf">November 13-19</h2>
							</div>
							<div class="col-md-4">
								<k:AssignView ui:field="mChairman" part="CHAIRMAN" />
								<k:AssignView ui:field="mPrayer1" part="PRAYER_1" />
								<k:AssignView ui:field="mPrayer2" part="PRAYER_2" />
							</div>
							<div class="col-md-4 b-r">
								<k:AssignView ui:field="mTreasures" part="TREASURES" />
								<k:AssignView ui:field="mDigging" part="DIGGING" />
								<k:AssignView ui:field="mLiving1" part="LIVING_1" />
								<k:AssignView ui:field="mLiving2" part="LIVING_2" />
								<k:AssignView ui:field="mCStudy" part="C_BIBLE_STUDY" />
							</div>
							<div class="col-md-4 b-r">
								<b.card:Card>
									<b.card:Front>
										<b.gwt:HTMLPanel>
											<label>Main Hall</label>
											<k:AssignView ui:field="mReading1" part="BIBLE_READING" />
											<k:AssignView ui:field="mInitial1" part="INITIAL_CALL" />
											<k:AssignView ui:field="mRv1" part="F_RETURN_VISIT" />
											<k:AssignView ui:field="mTalk1" part="TALK" />
										</b.gwt:HTMLPanel>
									</b.card:Front>
									<b.card:Back>
										<b.gwt:HTMLPanel>
											<label>Auxiliary Classroom</label>
											<k:AssignView ui:field="mReading2" part="BIBLE_READING" />
											<k:AssignView ui:field="mInitial2" part="INITIAL_CALL" />
											<k:AssignView ui:field="mRv2" part="F_RETURN_VISIT" />
											<k:AssignView ui:field="mTalk2" part="TALK" />
										</b.gwt:HTMLPanel>
									</b.card:Back>
								</b.card:Card>
							</div>
						</div>
					</b.gwt:HTMLPanel>

					<b.gwt:HTMLPanel ui:field="mStep2c" addStyleNames="content" visible="false">
						<div class="body">
							<div class="text-center m-t-md">
								<h2>Address information</h2>
							</div>
							<div class="col-md-6 b-r">
								<div class="form-group">
									<label>Address (1)</label>
									<k:AssignView part="CHAIRMAN" />
								</div>
								<div class="form-group">
									<label>City</label>
									<k:AssignView part="CHAIRMAN" />
								</div>
								<div class="form-group">
									<label>Zip</label>
									<k:AssignView part="CHAIRMAN" />
								</div>
							</div>
							<div class="col-md-6">
								<div class="form-group">
									<label>Address (2)</label>
									<k:AssignView part="CHAIRMAN" />
								</div>
								<div class="form-group">
									<label>State</label>
									<k:AssignView part="CHAIRMAN" />
								</div>
							</div>
						</div>
					</b.gwt:HTMLPanel>

					<b.gwt:HTMLPanel ui:field="mStep3c" addStyleNames="content" visible="false">
						<div class="body">
							<div class="text-center m-t-md">
								<h2>Congregation Information</h2>
							</div>
							<div class="col-md-6 b-r">
								<div class="form-group">
									<label>Field Service Group </label>
									<k:AssignView part="CHAIRMAN" />
								</div>
								<div class="form-group">
									<label>Baptized</label>
									<k:AssignView part="CHAIRMAN" />
								</div>
								<div class="form-group">
									<label>Roles</label>
									<k:AssignView part="CHAIRMAN" />
								</div>
								<div class="form-group">
									<label>Emergency</label>
									<k:AssignView part="CHAIRMAN" />
								</div>
								<div class="form-group">
									<label>Head Of House</label>
									<k:AssignView part="CHAIRMAN" />
								</div>
							</div>
							<div class="col-md-6">
								<div class="form-group">
									<label>Faith</label>
									<k:AssignView part="CHAIRMAN" />
								</div>
								<div class="form-group">
									<label>Started Publishing</label>
									<k:AssignView part="CHAIRMAN" />
								</div>
								<div class="form-group">
									<label>Categories</label>
									<k:AssignView part="CHAIRMAN" />
								</div>
								<div class="form-group">
									<label>Children</label>
									<k:AssignView part="CHAIRMAN" />
								</div>
								<div class="form-group">
									<label>Family</label>
									<k:AssignView part="CHAIRMAN" />
								</div>
							</div>
						</div>
					</b.gwt:HTMLPanel>

					<div class="actions">
						<ul>
							<li>
								<b:Anchor ui:field="mSave" text="Save" addStyleNames="strong" />
							</li>
						</ul>
					</div>
				</div>

			</div>
		</div>
	</b.gwt:HTMLPanel>
</ui:UiBinder>