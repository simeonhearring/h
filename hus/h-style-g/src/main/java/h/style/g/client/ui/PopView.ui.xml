<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:s="urn:import:vanderbilt.mc.style.client.ui">
	<ui:style>
		
	</ui:style>
	<g:PopupPanel ui:field="mPop" styleName="modal-dialog" animationEnabled="true" autoHideEnabled="false">
		<g:FlowPanel styleName="modal-content">
			<g:HTMLPanel styleName="modal-header">
				<s:ButtonView ui:field="mX" styleName="close" text="x" />
				<h4 class="modal-title">
					<g:InlineLabel ui:field="mHeader" />
				</h4>
			</g:HTMLPanel>
			<g:FlowPanel ui:field="mBody" styleName="modal-body" />
			<g:FlowPanel ui:field="mFooter" styleName="modal-footer">
				<s:ButtonView visible="false" ui:field="mClose" styleName="btn btn-default" text="Close" />
				<s:ButtonView visible="false" ui:field="mSave" styleName="btn btn-default" text="Save" />
			</g:FlowPanel>
		</g:FlowPanel>
	</g:PopupPanel>
</ui:UiBinder>